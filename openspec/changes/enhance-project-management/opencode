00130| /**
00131|  * è·å–çŠ¶æ€é¢œè‰²æ ·å¼
00132|  */
00133| const getStatusColor = (status: RequirementStatus) => {
00134|   switch (status) {
00135|     case 'draft':
00136|       return 'bg-gray-100 text-gray-800';
00137|     case 'review':
00138|       return 'bg-yellow-100 text-yellow-800';
00139|     case 'approved':
00140|       return 'bg-blue-100 text-blue-800';
00141|     case 'development':
00142|       return 'bg-purple-100 text-purple-800';
00143|     case 'testing':
00144|       return 'bg-orange-100 text-orange-800';
00145|     case 'completed':
00146|       return 'bg-green-100 text-green-800';
00147|     case 'rejected':
00148|       return 'bg-red-100 text-red-800';
00149|     default:
00150|       return 'bg-gray-100 text-gray-800';
00151|   }
00152| };
00153| 
00154| /**
00155|  * è·å–ä¼˜å…ˆçº§é¢œè‰²æ ·å¼
00156|  */
00157| const getPriorityColor = (priority: RequirementPriority) => {
00158|   switch (priority) {
00159|     case 'critical':
00160|       return 'bg-red-100 text-red-800 border-red-200';
00161|     case 'high':
00162|       return 'bg-orange-100 text-orange-800 border-orange-200';
00163|     case 'medium':
00164|       return 'bg-yellow-100 text-yellow-800 border-yellow-200';
00165|     case 'low':
00166|       return 'bg-green-100 text-green-800 border-green-200';
00167|     default:
00168|       return 'bg-gray-100 text-gray-800 border-gray-200';
00169|   }
00170| };
00171| 
00172| /**
00173|  * è·å–éœ€æ±‚ç±»å‹é¢œè‰²æ ·å¼
00174|  */
00175| const getTypeColor = (type: RequirementType) => {
00176|   switch (type) {
00177|     case 'feature':
00178|       return 'bg-blue-100 text-blue-800';
00179|     case 'enhancement':
00180|       return 'bg-green-100 text-green-800';
00181|     case 'bugfix':
00182|       return 'bg-red-100 text-red-800';
00183|     case 'research':
00184|       return 'bg-purple-100 text-purple-800';
00185|     default:
00186|       return 'bg-gray-100 text-gray-800';
00187|   }
00188| };
