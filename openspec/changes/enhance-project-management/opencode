00113|         <div className="flex gap-2">
00114|           <span
00115|             className={clsx(
00116|               "px-2 py-1 rounded-full text-xs font-medium",
00117|               priorityColors[task.priority],
00118|             )}
00119|           >
00120|             {t(`task.priority.${task.priority}`)}
00121|           </span>
00122|           <span
00123|             className={clsx(
00124|               "px-2 py-1 rounded-full text-xs font-medium",
