00250| /**
00251|  * éœ€æ±‚ç®¡ç†é¡µé¢
00252|  */
00253| export default function RequirementPage() {
00254|   const [requirements] = useState<Requirement[]>(generateSampleRequirements());
00255|   const [activeFilter, setActiveFilter] = useState('All');
00256| 
00257|   const filters = ['All', 'Draft', 'Review', 'Approved', 'Development', 'Testing', 'Completed'];
00258| 
00259|   const filteredRequirements = requirements.filter(requirement => {
00260|     if (activeFilter === 'All') return true;
00261|     return requirement.status === activeFilter.toLowerCase();
00262|   });
00263| 
00264|   return (
00265|     <div className="w-full">
00266|       <div className="mb-6">
