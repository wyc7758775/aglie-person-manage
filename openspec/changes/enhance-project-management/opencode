00053|   customer_id: string;
00054|   amount: number;
00055|   date: string;
00056|   // In TypeScript, this is called a string union type.
00057|   // It means that the "status" property can only be one of the two strings: 'pending' or 'paid'.
00058|   status: 'pending' | 'paid';
00059| };
00060| 
00061| export type Revenue = {
00062|   month: string;
00063|   revenue: number;
00064| };
00065| 
00066| export type LatestInvoice = {
00067|   id: string;
00068|   name: string;
00069|   image_url: string;
00070|   email: string;
00071|   amount: string;
00072| };
00073| 
00074| // The database returns a number for amount, but we later format it to a string with the formatCurrency function
00075| export type LatestInvoiceRaw = Omit<LatestInvoice, 'amount'> & {
00076|   amount: number;
00077| };
00078| 
00079| export type InvoicesTable = {
00080|   id: string;
00081|   customer_id: string;
00082|   name: string;
00083|   email: string;
00084|   image_url: string;
00085|   date: string;
00086|   amount: number;
00087|   status: 'pending' | 'paid';
00088| };
00089| 
00090| export type CustomersTableType = {
00091|   id: string;
00092|   name: string;
00093|   email: string;
00094|   image_url: string;
00095|   total_invoices: number;
00096|   total_pending: number;
00097|   total_paid: number;
00098| };
00099| 
00100| export type FormattedCustomersTable = {
00101|   id: string;
00102|   name: string;
00103|   email: string;
00104|   image_url: string;
00105|   total_invoices: number;
00106|   total_pending: string;
00107|   total_paid: string;
00108| };
00109| 
00110| export type CustomerField = {
00111|   id: string;
00112|   name: string;
00113| };
00114| 
00115| export type InvoiceForm = {
00116|   id: string;
00117|   customer_id: string;
00118|   amount: number;
00119|   status: 'pending' | 'paid';
00120| };
00121| 

